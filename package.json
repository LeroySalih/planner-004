{
  "name": "planner-004",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "npx playwright test",
    "record-test": "sh -c 'npx playwright codegen http://localhost:3000 --output tests/$1/$2.spec.ts' --",
    "db:pull": "bash ./bin/db_dump_schema.sh",
    "db:apply:local": "bash ./bin/db_apply_local.sh",
    "db:baseline": "bash -c 'mkdir -p supabase/migrations && touch supabase/migrations/00000000000000__baseline.sql && echo Baseline created'",
    "db:diff": "bash -c 'npx supabase db diff --schema public -f change.sql'",
    "db:push": "npx supabase db push --linked",
    "db:dump:data": "bash ./bin/db_dump_data.sh",
    "db:sync": "bash -c './bin/db_dump_schema.sh && ./bin/db_apply_local.sh && mkdir -p supabase/migrations && touch supabase/migrations/00000000000000__baseline.sql && echo Baseline created'",
    "dev:fresh": "bash -c './bin/dev_db_sync.sh && next dev --turbopack'",
    "dev:fresh:data": "bash -c 'DEV_IMPORT_DATA=1 ./bin/dev_db_sync.sh && next dev --turbopack'",
    "pm2:restart": "pm2 stop dino.mr-salih.org || true && pm2 delete dino.mr-salih.org || true",
    "pm2:start": "pm2 start npm --name dino.mr-salih.org -- run start"
  },
